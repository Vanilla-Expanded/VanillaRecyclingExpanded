<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<PipeSystem.ProcessDef>
		<defName>VRecyclingE_TrashToBricks</defName>
		<label>Trash to trashbricks</label>
		<description>Turns 10 trash into 25 trashbricks.</description>
		<ingredients>
			<li>
				<thing>VRecyclingE_Trash</thing>
				<countNeeded>10</countNeeded>
			</li>
		</ingredients>
		<ticks>60000</ticks>
		<results>
			<li>
				<thing>VRecyclingE_TrashBrick</thing>
				<count>25</count>
			</li>
		</results>
	</PipeSystem.ProcessDef>
	<PipeSystem.ProcessDef>
		<defName>VRecyclingE_SplitStabilizedAlloypack</defName>
		<label>Split stabilized alloypack</label>
		<description>Turns 1 stabilized alloypack into 1 Plasteel, 1 Gold and 2 Steel.</description>
		<ingredients>
			<li>
				<thing>VRecyclingE_StabilizedAlloypack</thing>
				<countNeeded>1</countNeeded>
			</li>
		</ingredients>
		<ticks>15000</ticks>
		<results>
			<li>
				<thing>Steel</thing>
				<count>2</count>
				<outputCellOffset>(1,0,-1)</outputCellOffset>
			</li>
			<li>
				<thing>Plasteel</thing>
				<count>1</count>
				<outputCellOffset>(0,0,-1)</outputCellOffset>
			</li>
			<li>
				<thing>Gold</thing>
				<count>1</count>
				<outputCellOffset>(-1,0,-1)</outputCellOffset>
			</li>
		</results>
	</PipeSystem.ProcessDef>
</Defs>
