	<ThingDef ParentName="BuildingBase">
		<defName>VRecyclingE_GarbageCompactor</defName>
		<label>garbage compactor</label>
		<description>tbd</description>
		<thingClass>VanillaRecyclingExpanded.Building_Compactor</thingClass>
		<tickerType>Normal</tickerType>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/GarbageCompactor/GarbageCompactor</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>Cutout</shaderType>
			<drawSize>(3,2)</drawSize>
			<shadowData>
				<volume>(1.95,0.5,1.3)</volume>
			</shadowData>
			<damageData>
				<rect>(0.3,0.1,2.4,1.8)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/GarbageCompactor/GarbageCompactor_MenuIcon</uiIconPath>
		<stuffCategories>
			<li>Stony</li>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>150</costStuffCount>
		<costList>
			<Steel>40</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>4500</WorkToBuild>
			<MaxHitPoints>300</MaxHitPoints>
			<Cleanliness>-20</Cleanliness>
			<Beauty>-10</Beauty>
			<Flammability>0</Flammability>
		</statBases>
		<size>(3,2)</size>
		<designationCategory>Biotech</designationCategory>
		<fillPercent>1</fillPercent>
		<passability>Impassable</passability>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>250</basePowerConsumption>
				<shortCircuitInRain>true</shortCircuitInRain>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Glower">
				<glowRadius>3.44</glowRadius>
				<glowColor>(217,112,33,0)</glowColor>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>12</heatPerSecond>
			</li>
			<li Class="VanillaRecyclingExpanded.CompProperties_SuperSimpleProcessor">
				<thingDef>VRecyclingE_Trash</thingDef>
				<stackLimit>10</stackLimit>
				<ejectIcon>UI/EjectContentFromGarbageCompactor</ejectIcon>
				<drawContainedThing>false</drawContainedThing>
				<ticksToFinish>7500</ticksToFinish>
				<contentsDrawOffset>(0.05, 0, 0.2)</contentsDrawOffset>
				<useEffecter>false</useEffecter>
				<results>
					<li>
						<thingResult>VRecyclingE_TrashBrick</thingResult>
						<count>2</count>
					</li>
				</results>
			</li>
		</comps>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<paintable>true</paintable>
		</building>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>VRecyclingE_ComplexRecycling</li>
		</researchPrerequisites>
	</ThingDef>
