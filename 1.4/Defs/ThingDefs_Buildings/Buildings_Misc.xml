<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>VRecyclingE_LargePollutionPump</defName>
		<label>large pollution pump</label>
		<description>A large, groundwater-filtering pump which slowly cleans large swathes of polluted terrain. The extracted pollution is formed into toxic wastepacks which must be hauled away.</description>
		<tickerType>Normal</tickerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Building/LargePollutionPump</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<fillPercent>0.2</fillPercent>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
		</building>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<designationCategory>Biotech</designationCategory>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>10000</WorkToBuild>
			<Mass>45</Mass>
			<MarketValue>500</MarketValue>
			<Flammability>0.5</Flammability>
		</statBases>
		<size>(3,3)</size>
		<costList>
			<Steel>180</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>400</basePowerConsumption>
			</li>
			<li Class="CompProperties_PollutionPump">
				<radius>30.9</radius>
				<pumpEffecterDef>PollutionPumped</pumpEffecterDef>
				<pumpsPerWastepack>6</pumpsPerWastepack>
				<intervalTicks>15000</intervalTicks>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_PollutionPump</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
	</ThingDef>
	<ThingDef ParentName="BuildingBase">
		<defName>VRecyclingE_PollutionFastPump</defName>
		<label>pollution fastpump</label>
		<description>A rapidly-working, groundwater-filtering pump which cleans polluted terrain. The extracted pollution is formed into toxic wastepacks which must be hauled away. It requires a large amount of electricity to work, but it works extremely quickly.</description>
		<tickerType>Normal</tickerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Building/PollutionFastPump</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<fillPercent>0.2</fillPercent>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
		</building>
		<constructionSkillPrerequisite>3</constructionSkillPrerequisite>
		<designationCategory>Biotech</designationCategory>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>5500</WorkToBuild>
			<Mass>15</Mass>
			<MarketValue>220</MarketValue>
			<Flammability>0.5</Flammability>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>75</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
			<Gold>1</Gold>
		</costList>
		<researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>800</basePowerConsumption>
			</li>
			<li Class="CompProperties_PollutionPump">
				<radius>10.9</radius>
				<pumpEffecterDef>PollutionPumped</pumpEffecterDef>
				<pumpsPerWastepack>6</pumpsPerWastepack>
				<intervalTicks>5000</intervalTicks>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_PollutionPump</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
	</ThingDef>
	<ThingDef ParentName="FurnitureBase">
		<defName>VRecyclingE_WasteCrate</defName>
		<label>waste crate</label>
		<description>A hermetically sealed container that prevents toxic waste stored inside from dissolving and leaking into the environment. Can only hold a limited amount of waste. Does nothing to prevent the waste from burning.</description>
		<thingClass>VanillaRecyclingExpanded.Building_WasteCrate</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/WasteCrate/WasteCrate_Bottom</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<uiIconPath>Things/Building/WasteCrate/WasteCrate_MenuIcon</uiIconPath>
		<costList>
			<Steel>40</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>5</Mass>
			<WorkToBuild>2500</WorkToBuild>
			<Flammability>0.5</Flammability>
			<MarketValue>60</MarketValue>
			<Beauty>0</Beauty>
		</statBases>
		<size>(1,1)</size>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.5</fillPercent>
		<designationCategory>Biotech</designationCategory>
		<castEdgeShadows>true</castEdgeShadows>
		<pathCost>42</pathCost>
		<rotatable>false</rotatable>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<building>
			<wastepackAtomizerBottomGraphic>
				<texPath>Things/Building/WasteCrate/WasteCrate_Base</texPath>
				<graphicClass>Graphic_Single</graphicClass>
				<drawSize>(2,2)</drawSize>
			</wastepackAtomizerBottomGraphic>
			<wastepackAtomizerWindowGraphic>
				<texPath>Things/Building/WasteCrate/WasteCrate_Top</texPath>
				<graphicClass>Graphic_Single</graphicClass>
				<shaderType>Transparent</shaderType>
				<drawSize>(2,2)</drawSize>
			</wastepackAtomizerWindowGraphic>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<maxItemsInCell>1</maxItemsInCell>
			<storageGroupTag>Shelf</storageGroupTag>
			<blueprintClass>Blueprint_Storage</blueprintClass>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<thingDefs>
						<li>Wastepack</li>
						<li>VRecyclingE_ReclaimedBiopack</li>
						<li>VRecyclingE_ReactiveChempack</li>
						<li>VRecyclingE_StabilizedAlloypack</li>
						<li>VRecyclingE_VolatileToxpack</li>
						<li>VRecyclingE_RadioactiveIsotopack</li>
						<li>VRecyclingE_Trash</li>
					</thingDefs>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Preferred</priority>
				<filter>
					<thingDefs>
						<li>Wastepack</li>
						<li>VRecyclingE_ReclaimedBiopack</li>
						<li>VRecyclingE_ReactiveChempack</li>
						<li>VRecyclingE_StabilizedAlloypack</li>
						<li>VRecyclingE_VolatileToxpack</li>
						<li>VRecyclingE_RadioactiveIsotopack</li>
						<li>VRecyclingE_Trash</li>
					</thingDefs>
				</filter>
			</defaultStorageSettings>
			<paintable>false</paintable>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>VRecyclingE_BasicRecycling</li>
		</researchPrerequisites>
		<placeWorkers>
			<li>VanillaRecyclingExpanded.PlaceWorker_WasteCrate</li>
		</placeWorkers>
	</ThingDef>
</Defs>