<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <RecipeDef>
    <defName>VRecyclingE_Recycle_WastepackIntoChempack</defName>
    <label>recycle wastepacks into reactive chempacks</label>
    <description>Use manual labor to recycle toxic wastepacks into reactive chempacks, creating reclaimed biopacks as a byproduct.</description>
    <jobString>Recycling wastepacks into reactive chempacks.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1200</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Wastepack</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Wastepack</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <VRecyclingE_ReactiveChempack>2</VRecyclingE_ReactiveChempack>
	  <VRecyclingE_ReclaimedBiopack>1</VRecyclingE_ReclaimedBiopack>
    </products>
    <workSkill>Crafting</workSkill>
    <displayPriority>0</displayPriority>
  </RecipeDef>
  
